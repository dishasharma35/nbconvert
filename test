    "import pandas as pd\n",
    "\n",
    "with open('merged_pc_enst_fpkm1', 'r') as f:\n",
    "    data = f.readlines()\n",
    "\n",
    "result = {}\n",
    "for line in data:\n",
    "    cols = line.rstrip('\\n').rstrip('\\r').split('\\t')\n",
    "    if cols[2] in result:\n",
    "        if cols[0] in result[cols[2]]:\n",
    "            print \"Hey something is fishy\"\n",
    "        else:\n",
    "            result[cols[2]][cols[0]] = cols[1]\n",
    "    else:\n",
    "        result[cols[2]] = {cols[0]: cols[1]}\n",
    "\n",
    "df = pd.DataFrame(result).T\n",
    "df.to_csv('disha_output.csv')"
